/*找财报，预览pdf功能*/
.pdfMasker{
	position: fixed;
	left: 0;
	bottom: -100%;
	background: rgba(0,0,0,0.5) /*rgba(95,189,211,0.5)*/;
	width: 100%;
	height: 100%;
	z-index: 1000;
	display: block;
	overflow: hidden;
}
.pdfView{
	position: absolute;
	left: 12%;
	top: 5%;
	width: 76%;
	height: 90%;
	z-index: 1001;
}
.pdfMasker .closePdf{
	position: absolute;
	top: -50px;
	right: -50px;
	width: 100px;
	height: 100px;
	border-radius: 50%;
	background: #333 url(../images/closePdfW.png) no-repeat;
	background-position: 22% 75%;
	z-index: 1002;
	cursor: pointer;
}
.pdfMasker .closePdf:hover{
	background: #EB4F38 url(../images/closePdfW.png) no-repeat;
	background-position: 22% 75%;
}
.pdfMasker ul li{
	float: left;
	color: #fff;
	margin:10px 15px;
}
.pdfMasker .pdf_btn{
	position: absolute;
	top: 50%;
	margin-top: -35px;
	width: 70px;
	height: 70px;
	border-radius: 50%;
	background: #ccc;
	color: #fff;
	border: 0;
	font-size: 30px;
}
.pdfMasker .pdf_prev{
	left: 5px;
}
.pdfMasker .pdf_next{
	right: 5px;
}
.pdfMasker .pdf_btn:hover{
	opacity: 0.5;
}
