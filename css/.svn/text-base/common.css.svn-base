/*封装的弹出框*/

.zmBg {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0);
	z-index: 1000;
	display: none;
}

.zmAlert {
	position: fixed;
	left: 50%;
	top: 25%;
	margin-left: -175px;
	width: 350px;
	background: #fff;
	border-radius: 5px;
	z-index: 1001;
	display: none;
	text-align: center;
	font-size: 22px;
	font-weight: bold;
	padding: 50px 0px;
	box-shadow: 0px 0px 3px #5fbdd3;
	color: #002657;
	border: 1px solid #5fbdd3;
}

.zmAlert p {
	font-size: 18px;
	height: 100%;
	width: 100%;
	font-weight: bold;
	color: #666;
}

.zmConfirm .zmclose,
.zmAlert>button {
	position: absolute;
	top: 5px;
	right: 5px;
	width: 20px;
	height: 20px;
	background: url(../images/alertClose.svg);
	border: 0;
	cursor: pointer;
}

.zmConfirm {
	position: fixed;
	left: 10%;
	top: 25%;
	width: 80%;
	height: 140px;
	background: #fff;
	border-radius: 5px;
	z-index: 1001;
	text-align: center;
	font-size: 22px;
	font-weight: bold;
	color: #002657;
	display: none;
}

.zmConfirm p {
	position: absolute;
	width: 80%;
	left: 10%;
	top: 40px;
}

.zmConfirm a {
	position: absolute;
	width: 50%;
	height: 40px;
	line-height: 40px;
	bottom: 0;
	background: #31b6e7;
	color: #fff;
}

.zmConfirm a:first-of-type {
	left: 0;
	border-radius: 0 0 0 5px;
}

.zmConfirm a:last-of-type {
	right: 0;
	border-radius: 0 0 5px;
	border-left: solid 1px #fff;
}


