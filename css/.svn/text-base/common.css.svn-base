	clear: both;
}


/*发送短信验证码按钮禁用状态*/

.disabled {
	background: #555;
	cursor: not-allowed;
}


/*模拟单选框、多选框*/

.data-radio input,
.data-checkbox input {
	display: none;
}

.data-radio,
.data-checkbox {
	display: inline-block;
	margin-right: 5px;
}

.data-radio .radio {
	position: relative;
	display: inline-block;
	width: 15px;
	height: 15px;
	border: 1px solid #666;
	background: #fff;
	border-radius: 50%;
	cursor: pointer;
	margin: 0 5px;
}

.data-radio .radio.on:after {
	content: '';
	width: 11px;
	height: 11px;
	background: #5c5c5c;
	border-radius: 50%;
	position: absolute;
	left: 1px;
	top: 1px;
}

.data-checkbox .checkbox {
	position: relative;
	display: inline-block;
	width: 15px;
	height: 15px;
	border: 1px solid #666;
	background: #fff;
	border-radius: 3px;
	cursor: pointer;
}

.data-checkbox .checkbox.on:after {
	content: '';
	width: 15px;
	height: 15px;
	background: url(../images/checkboxOn.png) no-repeat center;
	position: absolute;
	left: 1px;
	top: 0;
}

.data-checkbox .checkboxWord,
.data-radio .radioWord {
	position: relative;
	top: -2px;
	cursor: pointer;
}


