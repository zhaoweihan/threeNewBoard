/**行情列表**/

.quotation {
	width: 100%;
	margin: 0 auto;
	margin-top: 75px;
}

.quotation_tab {
	width: 900px;
	margin: 0 auto;
	margin-top: 30px;
	text-align: center;
}

.quotation_tab a {
	display: inline-block;
	font-size: 18px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	margin-right: 30px;
	color: #333;
}

.quotation_tab a.hover {
	border-bottom: 2px solid #5fbdd3;
}

.quotationList {
	width: 100%;
	margin: 20px auto;
}

.quotationList table {
	border: 1px solid #e5e5e5;
	border-collapse: collapse;
	width: 100%;
}

.quotationList table tr {
	background: #fff none repeat scroll 0 0;
}
.quotationList table tr:hover {
	background: #e3f1f4;
}
.quotationList table tr:nth-child(odd) {
	background: #f5fafd;
}

.quotationList table th {
	background-color: #eef2f3;
	border: 1px solid #e3e3e3;
	color: #555;
	font-size: 14px;
	font-weight: normal;
	height: 40px;
	line-height: 40px;
	text-align: center;
}

.quotationList table td {
	border-bottom: 1px solid #e3e3e3;
	border-left: 1px dotted #e3e3e3;
	font-size: 13px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	word-break: break-all;
	word-wrap: break-word;
	color: #333;
}

.quotationList table td a {
	color: #0092ef;
}

.quotationList table .daima {
	width: 90px;
}

.quotationList table .mingcheng {
	width: 125px;
}

.quotationList table .zxj {
	width: 110px;
}

.quotationList table .zde {
	width: 100px;
}

.quotationList table .zdf {
	width: 105px;
}

.quotationList table .jk {
	width: 98px;
}

.quotationList table .zuigao {
	width: 90px;
}

.quotationList table .zuidi {
	width: 90px;
}

.quotationList table .cjlwg {
	width: 150px;
}

.quotationList table .cjjew {
	width: 180px;
}

.quotationList table .ef {
	color: #0092ef;
	cursor: pointer;
}

.quotationList table .red {
	color: #cd6060;
}

.quotationList table .green {
	color: #009084;
}

.quotationList table .black {
	color: #333;
}

.more {
	width: 100%;
	margin: 0 auto;
	margin-top: 10px;
	border: 1px solid #ccc;
	box-sizing: border-box;
	background: #f5fafd;
}

.more a {
	display: block;
	width: 100%;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #999;
	font-size: 14px;
}

.quotationList table i {
	display: none;
}

.quotationList table .sort i {
	display: block;
	background: url(../images/arrow_1.png) center center no-repeat;
	display: inline-block;
	margin-left: 5px;
	vertical-align: middle;
	width: 9px;
