	width: 100%;
	margin: 0 auto;
	text-align: center;
}

.jstitle span {
	display: block;
	text-align: center;
	font-size: 36px;
	color: #333;
	height: 40px;
	line-height: 40px;
	font-weight: normal
}

.jstitle b {
	display: block;
	width: 65px;
	height: 1px;
	border-top: 1px solid #333;
	margin: 0 auto;
	margin-top: 10px;
}

.ksjs_box {
	width: 100%;
	margin: 0 auto;
	margin-top: 60px;
}

.ksjs_box ul li {
	width: 130px;
	float: left;
	margin-right: 75px;
	position: relative;
}

.ksjs_box ul li img {
	position: relative;
	top: 0;
	transition: top 0.3s;
	-webkit-transition: top 0.3s;
}

.ksjs_box ul li span {
	position: relative;
	bottom: 0;
}

.ksjs_box ul li.hover img {
	top: -30px;
}

.ksjs_box ul li:last-child {
	margin-right: 0px;
}

.ksjs_box ul li span {
	display: block;
	width: 100%;
	height: 30px;
	line-height: 30px;
	font-size: 18px;
	color: #666;
	text-align: center;
}


