.main_left ul li.hyyj a.hover{ background-position:16px -921px;}
.main_right{width:914px; float:left; border-left:1px solid #c3c3c3; margin-left:30px; box-sizing:border-box; padding-left:30px;}
.right_dbox{ width:865px;}
.right_nav{ width:100%; margin:0 auto; border:1px solid #ccc; box-sizing:border-box;}
.r_n_left{ width:370px; float:left;}
.r_n_left h2{ display:inline-block; font-size:24px; color:#6dc3ea; height:80px; line-height:80px; margin-left:28px;}
.r_n_left em{ display:inline-block; font-style:normal; font-size:18px; color:#858da1; height:80px; line-height:80px; margin-left:5px;}
.r_n_left a{ display:inline-block; width:87px; height:32px; line-height:32px; text-align:center; font-size:14px; color:#999; background:url(../images/jiahao.svg) 10px center no-repeat; border:1px solid #ccc; padding-left:22px; box-sizing:border-box; margin-left:20px; background-size:25px 25px;}
.r_n_left a.on{ background-color:#f2f2f2;}
.r_n_right{ float:right;}
.r_n_right a{ display:inline-block; width:204px; height:43px; line-height:43px; text-align:center; font-size:14px; color:#fff; box-sizing:border-box; border-radius:5px; margin-right:40px; margin-top:20px; padding-left:40px;}
.r_n_right a.cwdb{ background:url(../images/bingzhuangtu.svg) 33px center no-repeat; background-color:#42b3e5; background-size:25px 25px;}
.r_n_right a:hover.cwdb{ background-color:#0384ce;}
.r_n_right a.wnldb{ background:url(../images/zhexian.svg) 33px center no-repeat; background-color:#3156a3; background-size:25px 25px;}
.r_n_right a:hover.wnldb{ background-color:#406ac1;}
.main_r_center{ width:100%; margin:0 auto;}
.r_center_title{ width:100%; height:50px; line-height:50px; border-bottom:1px solid #b6b6b6; margin-top:10px;}
.r_center_title span{ display:block; width:100%; text-align:left; font-size:20px; color:#474747;height:50px; line-height:50px;}
.r_center_box{ width:100%; margin:0 auto;}
.center_title{ width:100%; margin:0 auto; padding-bottom:28px;}
.center_title a{ display:inline-block; padding:5px 13px; margin-top:28px; font-size:16px;}
.center_title a.hover{ background-color:#3156a3; color:#fff; border-radius:5px;}

.center_table{ width:100%; margin:0 auto;}
.center_table table{border: 1px solid #e5e5e5;border-collapse: collapse; width: 100%;}
.center_table table tr{background: #fff none repeat scroll 0 0;}
.center_table table tr:nth-child(odd){ background:#f9f9f9;}
.center_table table tr:hover{ background:#e3f1f4;}
.center_table table th{background-color: #f9f9f9;border: 1px solid #e3e3e3;color: #555;font-size: 14px;font-weight:bold;height: 40px; line-height:40px;text-align: center;}
.center_table table td{  border-bottom: 1px solid #e3e3e3;border-left: 1px dotted #e3e3e3;font-size: 13px;height: 40px;line-height:40px;text-align: center;word-break: break-all; word-wrap: break-word; color:#333;}
.center_table table td a{ color:#5fbdd3;}
.center_table table th.kemu,.center_table table td.kemu{ width:200px;}
.center_table table th.c_time,.center_table table td.c_time{ width:158px;}
/**杜邦分析**/
.dbfx_box{ width:100%; margin-top:20px;}
.dbfx_box img{ display:block; width:100%; margin:0 auto;}
/**行业地位分析**/
.hydwfx_box{ width:100%; margin-top:20px;}
.hydwfx_box img{ display:block; width:100%; margin:0 auto;}
.table_title {
	width: 100%;
	margin: 0 auto;
}

.table_title p {
	display: inline-block;
	font-size: 18px;
	color: #666;
	box-sizing: border-box;
	padding-left: 25px;
}

.table_title p span {
	display: inline-block;
	font-size: 18px;
	color: #3984c3;
}

.table_title p span i {
	display: inline-block;
	font-size: 18px;
	font-style: normal;
}

.xuan_time {
	width: 220px;
	float: right;
	position: relative;
}

.xuan_time span {
	display: inline-block;
	float: left;
	color: #3984c3;
	font-size: 14px;
	height: 28px;
	line-height: 28px;
	margin-right: 20px;
}

#selectVal {
	width: 150px;
	height: 28px;
	float: left;
	border: 1px solid #ccc;
	line-height: 28px;
	padding-left: 6px;
	font-size: 14px;
	color: #666;
	box-sizing: border-box;
}

#selectList {
	width: 150px;
	height: auto;
	position: absolute;
	right: 8px;
	top: 27px;
	border: 1px solid #ccc;
	display: none;
	overflow: hidden;
	background: #FFF;
	box-sizing: border-box;
}

#selectList li {
	float: left;
	width: 100%;
	height: 28px;
	line-height: 28px;
	font-size: 14px;
	color: #9FA0A0;
	text-align: left;
	padding-left: 10px;
	box-sizing: border-box;
}

#selectList li:hover {
	background: #39A6FF;
	color: #fff;
	cursor: pointer;
}

.click3 {
	background: url("../images/dsj.svg") center center no-repeat;
	float: right;
	height: 28px;
	margin-right: 5px;
	width: 12px;
	background-size: 12px 12px;
	position: absolute;
	top: 0px;
	right: 10px;
	cursor: pointer;
}
.hydw_sjb{ width:100%; margin:0 auto; }
.hydw_sjb img{ display:block; width:100%; margin:0 auto; margin-top:20px;}
.cwfx_box{width: 100%; margin: 0 auto;}
.cwfx_box h2{width: 100%; margin: 0 auto; font-size: 16px; line-height: 30px; font-weight: bold;margin-top: 15px;}
.cwfx_box ul{width: 100%; margin: 0 auto;margin-top: 15px;}
.cwfx_box ul li{width: 100%; line-break: 24px; margin-top: 10px;}
.cwfx_box ul li span{display: inline-block; font-size: 14px; color: #5c5c5c; line-height: 24px;}
.cwfx_box ul li p{ display: inline-block;font-size: 14px; color: #333; line-height: 24px;}

/**公司公告**/
.gsgg_box{width: 100%; margin: 0 auto;}
.gsgg_box ul li{width: 100%; margin: 0 auto; height: 58px; line-height: 58px; border-top:1px solid #e6e6e6;}
.gsgg_box ul li:last-child{border-bottom: 1px solid #e6e6e6;}
.gsgg_box ul li:nth-child(odd){background:#f5f5f5;}
