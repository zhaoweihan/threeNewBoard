.main_left ul li a{ display:block; width:100%; height:65px; line-height:65px; background:url(../images/leftbj.png) 1px center no-repeat; text-align:center; box-sizing:border-box;}
.main_left ul li.qyzl a{ background-position:15px 1px;}
.main_left ul li.qyzl a:hover{background-position:16px -498px; }
.main_left ul li.qyzl a.hover{ background-position:16px -498px;}
.main_left ul li.gsgg a{ background-position:15px -72px;}
.main_left ul li.gsgg a:hover{background-position:16px -570px;}
.main_left ul li.gsgg a.hover{ background-position:16px -570px;}
.main_left ul li.gbgd a{ background-position:15px -143px;}
.main_left ul li.gbgd a:hover{background-position:16px -642px; }
.main_left ul li.gbgd a.hover{ background-position:16px -642px;}
.main_left ul li.cwsj a{ background-position:15px -210px;}
.main_left ul li.cwsj a:hover{background-position:16px -709px; }
.main_left ul li.cwsj a.hover{ background-position:16px -709px;}
.main_left ul li.fxfp a{ background-position:15px -280px;}
.main_left ul li.fxfp a:hover{background-position:16px -779px; }
.main_left ul li.fxfp a.hover{ background-position:16px -779px;}
.main_left ul li.yqjc a{ background-position:15px -347px;}
.main_left ul li.yqjc a:hover{background-position:16px -847px; }
.main_left ul li.yqjc a.hover{ background-position:16px -847px;}
.main_left ul li.hyyj a{ background-position:15px -422px;}
